{
  "name": "csgo-discord-bot",
  "version": "1.0.0",
  "description": "Bot de Discord tipo Karuta con sistema de cajas CS:GO/CS2",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "deploy-commands": "tsx src/bot/deploy-commands.ts",
    "seed": "tsx src/db/seedCS2Cases.ts && tsx scripts/updateShopItems.ts",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx src/db/seed.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "csgo",
    "cs2",
    "karuta",
    "cases",
    "economy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "axios": "^1.13.2",
    "bullmq": "^5.1.0",
    "cheerio": "^1.1.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
